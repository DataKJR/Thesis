	author = {Loucks, A. B. and Heath, E. M.},
	month = mar,
	year = {1994},
	pmid = {8160876},
	keywords = {Exercise, Female, Humans, Adult, Energy Metabolism, Models, Biological, Regression Analysis, Analysis of Variance, Differential Threshold, Syndrome, Triiodothyronine},
	pages = {R817--823},
}

