@article{teslim_intra-tester_1991,
	title = {Intra-tester {Reliability} and {Validity} on the {Takei} {Kigi} {Kogyo} {Hand} {Grip} {Dynamometer}},
	volume = {Vol. 3},
	abstract = {Abstract. The purposes of this study were to determine the reliability and validity of the Takeki Kiki Kogyo dynamometer. Using the test-retest experimental design, we measured the right and left hand grip strength of ten male subjects on two different occasions within one week interval. The results revealed that the dynamometer reading is highly reproducible both for the right (r=0.91, p{\textless}0.01) and the left(r=0.96, p{\textless}0.01) upper extremities. The measurement on day 1 is not significantly (p{\textgreater}0.05) different from the day 2 measurement. We established the validity of the dynamometer by recording the output of the instrument when known weights were suspended from the end of the dynamometer. The validity correlation coefficients computed were all greater than 0.90 (p{\textless}0.01). Furthermore, the pooled data set for the standard weight is not significantly (p{\textgreater}0.05) different from the dynamometer output.
Key Words: dynamometry, grip strength, functional assessment, tests and},
