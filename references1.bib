@article{wilson-barnes_relationship_2021,
	title = {The relationship between vitamin {D} status, intake and exercise performance in {UK} {University}-level athletes and healthy inactive controls},
	volume = {16},
	issn = {1932-6203},
	doi = {10.1371/journal.pone.0249671},
	abstract = {The potential ergogenic effects of vitamin D (vitD) in high performing athletes has received considerable attention in the literature and media. However, little is known about non-supplemented university athletes and students residing at a higher latitude. This study aimed to investigate the effects of vitD (biochemical status and dietary intake) on exercise performance in UK university athletes and sedentary students. A total of 34 athletes and 16 sedentary controls were studied during the spring and summer months. Serum vitD status and sunlight exposure were assessed using LC-MS/MS and dosimetry, respectively. Muscular strength of the upper and lower body was assessed using handgrip and knee extensor dynamometry (KE). Countermovement jump (CMJ) and aerobic fitness were measured using an Optojump and VO2max test, respectively. Statistical analysis was performed using paired/ independent t-tests, ANCOVA and Pearson/ Spearman correlations, depending on normality. VitD status increased significantly over the seasons, with athletes measuring higher status both in spring (51.7±20.5 vs. 37.2±18.9 nmol/L, p = 0.03) and summer (66.7±15.8 vs 55.6±18.8 nmol/L, p = 0.04) when compared to controls, respectively. Notably, 22\% of the subjects recruited were vitD deficient during the spring term only ({\textless}25nmol/L, n 9). Subjects with 'insufficient' vitD status ({\textless}50nmol/L) elicited significantly lower CMJ when contrasted to the vitD 'sufficient' ({\textgreater}50nmol/l) group (p = 0.055) and a lower VO2 max (p = 0.05) in the spring and summer term (p = 0.05 and p = 0.01, respectively). However, an ANCOVA test showed no significant difference detected for either CMJ or VO2max following adjustments for co-variates. In conclusion, we provide novel information on the vitD status, dietary intake, physical fitness and sunlight exposure of UK young adults across two separate seasons, for which there is limited data at present.},
	language = {eng},
	number = {4},
	journal = {PloS One},
	author = {Wilson-Barnes, Saskia L. and Hunt, Julie E. A. and Mendis, Jeewaka and Williams, Emma L. and King, David and Roberts, Harry and Lanham-New, Susan A. and Manders, Ralph J. F.},
	year = {2021},
	pmid = {33798240},
	pmcid = {PMC8018647},
	keywords = {Athletes, Exercise, Female, Humans, Male, Vitamin D, Vitamin D Deficiency, Adult, Universities, Sunlight, Dietary Supplements, Muscle Strength, Nutritional Status, Hand Strength, Chromatography, Liquid, Seasons, Sedentary Behavior, Tandem Mass Spectrometry, United Kingdom},
	pages = {e0249671},
	file = {Full Text:/Users/Katriona/Zotero/storage/U2WB7UTS/Wilson-Barnes et al. - 2021 - The relationship between vitamin D status, intake .pdf:application/pdf},
}

@article{rago_countermovement_2018,
	title = {Countermovement {Jump} {Analysis} {Using} {Different} {Portable} {Devices}: {Implications} for {Field} {Testing}},
	volume = {6},
	issn = {2075-4663},
	shorttitle = {Countermovement {Jump} {Analysis} {Using} {Different} {Portable} {Devices}},
	doi = {10.3390/sports6030091},
	abstract = {The aim of this study was to analyze the concurrent validity, test⁻retest reliability, and capacity to detect changes of four different portable devices used to measure a wide range of neuromuscular parameters derived from countermovement jump (CMJ). An accelerometric device (Myotest), a jump mat (Ergojump), an optical device (Optojump), and a smartphone app (MyJump) were simultaneously examined for concurrent validity against gold-standard measures (motion-capture system and a force platform). Twenty-two CMJ-derived variables were collected from 15 healthy male subjects (n = 60 CMJs). Contraction time (CT) and eccentric duration (EccD) measurements obtained from the Myotest were moderately to largely associated with and not different from force platform measurements (r = 0.31 to 0.64, ES = 0.11 to 0.18) and showed moderate test-retest reliability (intraclass correlation coefficient (ICC) = 0.92 to 0.97, coefficient of variation (CV) = 3.8 to 8.0\%). Flight time (FT) and jump height (JH) from Ergojump, Optojump, and MyJump showed moderate to strong associations with gold-standard measurements (r = 0.57 to 0.98) and good test⁻retest reliability (ICC = 0.54 to 0.97, CV = 1.8 to 4.2). However, all portable devices underestimated JH (ES = 1.25 to 2.75). Independent of the instrument used, the analyzed CMJ variables showed good capacity to detect changes (standard error of measurement (SEM) {\textless} smallest worthwhile change (SWC)), with the exception of rate of force and rate of power development parameters, which showed marginal capacity (SEM {\textgreater} SWC). The Myotest is preferable to measure temporal parameters during ground contact, whereas Ergojump, Optojump, and MyJump devices may be preferable to measure FT and JH, with the Optojump being the most accurate.},
	language = {eng},
	number = {3},
	journal = {Sports (Basel, Switzerland)},
	author = {Rago, Vincenzo and Brito, João and Figueiredo, Pedro and Carvalho, Thiago and Fernandes, Tiago and Fonseca, Pedro and Rebelo, António},
	month = aug,
	year = {2018},
	pmid = {30200384},
	pmcid = {PMC6162675},
	keywords = {force platform, measurements, motion analysis, neuromuscular assessment, reliability, validity},
	pages = {91},
	file = {Full Text:/Users/Katriona/Zotero/storage/V7D3BBDV/Rago et al. - 2018 - Countermovement Jump Analysis Using Different Port.pdf:application/pdf},
}

@article{glatthorn_validity_2011,
	title = {Validity and reliability of {Optojump} photoelectric cells for estimating vertical jump height},
	volume = {25},
	issn = {1533-4287},
	doi = {10.1519/JSC.0b013e3181ccb18d},
	abstract = {Vertical jump is one of the most prevalent acts performed in several sport activities. It is therefore important to ensure that the measurements of vertical jump height made as a part of research or athlete support work have adequate validity and reliability. The aim of this study was to evaluate concurrent validity and reliability of the Optojump photocell system (Microgate, Bolzano, Italy) with force plate measurements for estimating vertical jump height. Twenty subjects were asked to perform maximal squat jumps and countermovement jumps, and flight time-derived jump heights obtained by the force plate were compared with those provided by Optojump, to examine its concurrent (criterion-related) validity (study 1). Twenty other subjects completed the same jump series on 2 different occasions (separated by 1 week), and jump heights of session 1 were compared with session 2, to investigate test-retest reliability of the Optojump system (study 2). Intraclass correlation coefficients (ICCs) for validity were very high (0.997-0.998), even if a systematic difference was consistently observed between force plate and Optojump (-1.06 cm; p {\textless} 0.001). Test-retest reliability of the Optojump system was excellent, with ICCs ranging from 0.982 to 0.989, low coefficients of variation (2.7\%), and low random errors (±2.81 cm). The Optojump photocell system demonstrated strong concurrent validity and excellent test-retest reliability for the estimation of vertical jump height. We propose the following equation that allows force plate and Optojump results to be used interchangeably: force plate jump height (cm) = 1.02 × Optojump jump height + 0.29. In conclusion, the use of Optojump photoelectric cells is legitimate for field-based assessments of vertical jump height.},
	language = {eng},
	number = {2},
	journal = {Journal of Strength and Conditioning Research},
	author = {Glatthorn, Julia F. and Gouge, Sylvain and Nussbaumer, Silvio and Stauffacher, Simone and Impellizzeri, Franco M. and Maffiuletti, Nicola A.},
	month = feb,
	year = {2011},
	pmid = {20647944},
	keywords = {Female, Humans, Male, Sports, Adult, Young Adult, Cohort Studies, Athletic Performance, Exercise Test, Muscle Strength, Reproducibility of Results, Equipment Design, Sensitivity and Specificity},
	pages = {556--560},
}

@article{blake_role_2007,
	title = {The role of {DXA} bone density scans in the diagnosis and treatment of osteoporosis},
