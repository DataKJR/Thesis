	pmid = {15333731},
	keywords = {Energy Intake, Female, Humans, Male, Middle Aged, Adult, Diet, Adolescent, Cohort Studies, Cross-Sectional Studies, Eating, Feeding Behavior, Nutritional Physiological Phenomena, Surveys and Questionnaires},
	pages = {2372--2380},
}

