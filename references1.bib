	journal = {The International Journal of Behavioral Nutrition and Physical Activity},
	author = {Anglé, Susanna and Engblom, Janne and Eriksson, Tiina and Kautiainen, Susanna and Saha, Marja-Terttu and Lindfors, Pirjo and Lehtinen, Matti and Rimpelä, Arja},
